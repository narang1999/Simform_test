<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/postgraph"
    app:startDestination="@id/users">
    <fragment
        android:id="@+id/users"
        android:name="com.example.simform_test.view.main.Users"
        android:label="Users" >
        <action
            android:id="@+id/action_users_to_userDetail"
            app:destination="@id/userDetail" />
    </fragment>
    <fragment
        android:id="@+id/userDetail"
        android:name="com.example.simform_test.view.main.UserDetail"
        android:label="UserDetail" >
        <argument
            android:name="userData"
            app:argType="com.example.simform_test.models.Example" />

    </fragment>

</navigation>